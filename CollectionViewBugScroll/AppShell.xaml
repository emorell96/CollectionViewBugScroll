<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CollectionViewBugScroll.AppShell"
       xmlns:local="clr-namespace:CollectionViewBugScroll"
           
       
             Title="AppShell"
             >
    <TabBar>
        <Tab Title="Bugged"
             >
            <ShellContent Title="Bug please"
                          ContentTemplate="{DataTemplate local:MainPage}" />
            <ShellContent Title="Retailer Options"
                          ContentTemplate="{DataTemplate local:MainPage}" />
            <ShellContent Title="Active Notifications"
                          ContentTemplate="{DataTemplate local:MainPage}"/>
        </Tab>
        <Tab Title="Less bugged">
            <ShellContent Title="Main" ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
    </TabBar>
    
    
    
</Shell>